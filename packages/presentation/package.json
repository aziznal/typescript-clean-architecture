{
    "name": "presentation",
    "version": "0.0.0",
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "watch": "npm-watch",
        "test": "react-scripts test --env=jsdom",
        "test:watch": "jest --watch"
    },
    "watch": {
        "build": {
            "patterns": [
                "src"
            ],
            "extensions": "js,jsx,ts,tsx"
        }
    },
    "private": true,
    "dependencies": {
        "nodemon": "^3.0.1",
        "npm-watch": "^0.11.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router": "^6.15.0",
        "react-scripts": "^5.0.1",
        "react-testing-library": "^8.0.1",
        "rxjs": "~7.5.0",
        "tslib": "^2.3.0",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/jasmine": "~4.0.0",
        "@types/jest": "^28.1.6",
        "@types/react": "^18.2.20",
        "@types/react-dom": "^18.2.7",
        "jasmine-core": "~4.1.0",
        "jest": "^28.1.3",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.0.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "ts-jest": "^28.0.7",
        "typescript": "~4.7.2"
    },
    "nx": {
        "targets": {
            "build": {
                "outputs": [
                    "{projectRoot}/dist"
                ]
            }
        }
    }
}
